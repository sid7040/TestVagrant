<?xml version="1.0" encoding="UTF-8" ?>
<extentreports>
    <configuration>
        <theme>dark</theme>
        <protocol>http</protocol>
        <encoding>UTF-8</encoding>
        <documentTitle>Test Report</documentTitle> <!-- This title will be displayed as browser title for the page -->
        <reportName>Cucumber Test Run Report</reportName> <!-- This will be your report heading -->
        <!--<reportHeadline>A small headline</reportHeadline>-->
        <!-- global date format override -->  <!-- defaults to yyyy-MM-dd -->
        <dateFormat>dd-MM-yyyy</dateFormat>

        <!-- global time format override -->   <!-- defaults to HH:mm:ss -->
        <timeFormat>HH:mm:ss</timeFormat>
        <scripts>
            <![CDATA[
                $(document).ready(function() {
                $(‘#parentWithoutNodes’).prop("checked", true).click();
                $(‘#test-count-setting > div.modal-footer > a’)[0].click();
                });
                ]]>
        </scripts>
    </configuration>
</extentreports>